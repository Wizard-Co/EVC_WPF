                        <Grid Grid.Row="2" Grid.RowSpan="2" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid Grid.Column="0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Button x:Name="btnAllCheck" Grid.Row="0" Grid.Column="0" Visibility="Hidden"
                            HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                            Click="btnAllCheck_Click">
                                    <TextBlock Text="전체 선택" Margin="-2"/>
                                </Button>

                                <Button x:Name="btnAllNone" Grid.Row="1" Grid.Column="0" Visibility="Hidden"
                            HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                            Click="btnAllNone_Click">
                                    <TextBlock Text="선택 해제" Margin="-2"/>
                                </Button>

                                <Button x:Name="btnFinal" Grid.Row="0" Grid.Column="2" Grid.RowSpan="2" Content="확인" Margin="1" Visibility="Hidden"
                            HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Click="BtnFinal_Click" />
                            </Grid>

                            <Grid Grid.Column="0" Grid.ColumnSpan="3">
                                <DataGrid x:Name="dgdSum" IsReadOnly="True" SizeChanged="DataGrid_SizeChange" Style="{DynamicResource DataGridSet1}">
                                    <DataGrid.RowStyle>
                                        <Style TargetType="DataGridRow">
                                            <Style.Resources>
                                                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#3399FF"/>
                                                <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="White"/>
                                                <SolidColorBrush x:Key="{x:Static SystemColors.ControlBrushKey}" Color="#3399FF" />
                                                <SolidColorBrush x:Key="{x:Static SystemColors.ControlTextBrushKey}" Color="White"/>
                                            </Style.Resources>
                                        </Style>
                                    </DataGrid.RowStyle>
                                    <DataGrid.Columns>

                                        <DataGridTemplateColumn  Header="건수" Width="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                        <TextBlock Text="{Binding Count,StringFormat={}{0:#\,#}}" />
                                                        <TextBlock Text=" 건" />
                                                    </StackPanel>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn  Header="금액" Width="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding mtrAmount,StringFormat={}{0:#\,#}}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>

                                        <DataGridTemplateColumn  Header="수주정산소요일(평균)" MinWidth="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding dateDiffSum,StringFormat={}{0:#\,#}}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn  Header="처리건수(EA)" Width="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding orderSum,StringFormat={}{0:#\,#}}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                        <DataGridTemplateColumn  Header="인당처리건수(EA)" Width="100">
                                            <DataGridTemplateColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Text="{Binding PersonSum,StringFormat={}{0:#\,#}}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                </DataTemplate>
                                            </DataGridTemplateColumn.CellTemplate>
                                        </DataGridTemplateColumn>
                                    </DataGrid.Columns>
                                </DataGrid>

                            </Grid>
                        </Grid>


                    </Grid>

                </Grid>





                <!--서브 데이터그리드-->




            </Grid>
        </Grid>
    </Grid>
</UserControl>

