                                <!--==================================절취선=================================-->
                                <!--■영업회사■-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo28, Path=ActualWidth}" />
                                <!--영업담당자(연락처)-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo29, Path=ActualWidth}" />
                                <!--영업부담금추가공사-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo30, Path=ActualWidth}" />
                                <!--사은품정보-->

                                <!--==================================절취선=================================-->
                                <!--■기기 및 액세사리 정보■-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo31, Path=ActualWidth}" />
                                <!--모델명-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo32, Path=ActualWidth}" />
                                <!--충전기발주-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo33, Path=ActualWidth}" />
                                <!--충전기입고-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo34, Path=ActualWidth}" />
                                <!--입고수량-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo35, Path=ActualWidth}" />
                                <!--충전기입고지정보-->
                                 
                                <ColumnDefinition Width="{Binding ElementName=orderNo36, Path=ActualWidth}" />
                                <!--스탠드&캐노피 발주업체-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo37, Path=ActualWidth}" />
                                <!--헬멧형-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--실내폴-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--일체형-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--스탠드발주일-->
                                
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--스탠드입고일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--캐노피&스탠드 입고지정-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--캐노피&스탠드 발주금-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--비고-->

                                <!--==================================절취선=================================-->
                                <!--■시공및 실사정보■-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--실사요청일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--실사일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--실사횟수-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--실사자료접수일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--거점수-->
                                
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--전기조달방식-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--감리배치여부-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--추가공사비(시공사부담)-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--추가공사비(EVC부담)-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--실사처리비고-->

                                <!--==================================절취선=================================-->
                                <!--■공단사항■-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--공단접수번호-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--공단승인읿-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--공단마감일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--공기최종마감일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--공단사항비고-->
                                
                                <!--==================================절취선=================================-->
                                <!--■기기 한전수전정보■-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--거점수-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--전기조달방식-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--외선공사내용-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--시설부담금-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--운영사전달(시설부담금)-->
                                
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--전기사용신청일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--인입승인여부-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--인입승인일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--계량기설치정보-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--한전지사(연락처)-->
                                
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--접수증(고객번호)-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--한전불입금납부일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--계량기부설일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--한전수전 비고-->

                                <!--==================================절취선=================================-->
                                <!--■시공사정보■-->
                                
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--시공사업체-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--착공지시일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--착공일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--지연사유-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--시공완료일-->
                                <ColumnDefinition Width="{Binding ElementName=orderNo38, Path=ActualWidth}" />
                                <!--시공비고-->

