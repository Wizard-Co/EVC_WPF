<UserControl x:Class="WizMes_EVC.Win_ord_OrderClose_U"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WizMes_EVC"
             mc:Ignorable="d" Loaded="UserControl_Loaded"
             d:DesignHeight="620" d:DesignWidth="820">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="88*" />
            <RowDefinition Height="532*" />
        </Grid.RowDefinitions>
        <!-- 전체 중 상단 -->
        <Grid Grid.Row="0" Background="{DynamicResource WizMes_EVC}" Margin="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*" />
                <ColumnDefinition Width="160*" />
                <ColumnDefinition Width="230*" />
                <ColumnDefinition Width="230*" />
                <ColumnDefinition Width="100*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="10*" />
                <RowDefinition Height="10*" />
                <RowDefinition Height="10*" />
                <RowDefinition Height="10*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <!-- 상단의 날짜 컨트롤 -->
            <Grid Grid.Row="0" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*" />
                    <ColumnDefinition Width="85*" />
                </Grid.ColumnDefinitions>
                <Label x:Name="lblOrderDay" Grid.Column="0" Style="{DynamicResource LabelSearch}"
                       MouseLeftButtonUp="lblOrderDay_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkOrderDay" Checked="chkOrderDay_Checked" 
                                      Unchecked="chkOrderDay_Unchecked" IsChecked="True" />
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text=" 수주일자" Style="{DynamicResource TextBlockInViewBoxSearch}" />
                        </Viewbox>

                    </StackPanel>
                </Label>
                <DatePicker x:Name="dtpSDate" Grid.Column="1" Style="{DynamicResource DatePickerSearch}" IsEnabled="True"/>
            </Grid>
            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*" />
                    <ColumnDefinition Width="85*" />
                </Grid.ColumnDefinitions>
                <DatePicker x:Name="dtpEDate" Grid.Column="1" Style="{DynamicResource DatePickerSearch}" IsEnabled="True"/>
            </Grid>
            <Grid Grid.Row="2" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Button x:Name="btnLastMonth" Grid.Column="0" VerticalContentAlignment="Center" 
                        HorizontalContentAlignment="Center" Click="btnLastMonth_Click">
                    <TextBlock Text="전월" />
                </Button>
                <Button x:Name="btnThisMonth" Grid.Column="1" VerticalContentAlignment="Center" 
                        HorizontalContentAlignment="Center" Click="btnThisMonth_Click">
                    <TextBlock Text="금월" />
                </Button>
                <Button x:Name="btnYesterDay" Grid.Column="2" VerticalContentAlignment="Center" 
                        HorizontalContentAlignment="Center" Click="BtnYesterDay_Click">
                    <TextBlock Text="전일"/>
                </Button>
                <Button x:Name="btnToday" Grid.Column="3" VerticalContentAlignment="Center" 
                        HorizontalContentAlignment="Center" Click="btnToday_Click">
                    <TextBlock Text="금일" />
                </Button>

            </Grid>
            <Grid Grid.Row="3" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="65*" />
                    <ColumnDefinition Width="85*" />
                </Grid.ColumnDefinitions>
                <Label Grid.Column="0" HorizontalContentAlignment="Center" 
                       VerticalContentAlignment="Center">
                    <TextBlock Text="마감 여부" Margin="-2"/>
                </Label>
                <ComboBox x:Name="cboOrderStatus" Grid.Column="2" Background="White" IsEnabled="True"
                          SelectionChanged="cboOrderStatus_SelectionChanged"  Style="{DynamicResource ComboBoxSearchString}"/>
            </Grid>
            <!--<TextBlock Grid.Row="0" Grid.Column="4" Margin="1" Text="부터" VerticalAlignment="Center"/>-->
            <!--<TextBlock Grid.Row="1" Grid.Column="4" Margin="1" Text="까지" VerticalAlignment="Center"/>-->
            <!--// 상단의 날짜 컨트롤 -->



            <Grid Grid.Column="2" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="chkManageCustom_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkManageCustomSrh" Checked="chkManageCustom_Checked" Unchecked="chkManageCustom_UnChecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="운 영 사" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>

                    </StackPanel>
                </Label>
                <TextBox x:Name="txtManageCustomSrh" Grid.Column="1" Style="{DynamicResource TextBoxSearch}" IsEnabled="False" KeyDown="txtManageCustom_KeyDown"/>
                <Button x:Name="btnManageCustomSrh"  Grid.Column="2"  Click="btnManageCustom_Click" IsEnabled="False">
                    <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                </Button>
            </Grid>


            <Grid Grid.Column="2" Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="chkSalesCustom_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkSalesCustomSrh" Checked="chkSalesCustom_Checked" Unchecked="chkSalesCustom_UnChecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="영 업 사" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>

                    </StackPanel>
                </Label>
                <TextBox x:Name="txtSalesCustomSrh" Grid.Column="1" Style="{DynamicResource TextBoxSearch}" IsEnabled="False" KeyDown="txtSalesCustom_KeyDown"/>
                <Button x:Name="btnSalesCustomSrh"  Grid.Column="2"  Click="btnSalesCustom_Click" IsEnabled="False">
                    <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                </Button>
            </Grid>

            <Grid Grid.Column="2" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="chkArticleId_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkArticleIdSrh" Checked="chkArticleId_Checked" Unchecked="chkArticleId_UnChecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="제 품 명" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>

                    </StackPanel>
                </Label>
                <TextBox x:Name="txtArticleIdSrh" Grid.Column="1" Style="{DynamicResource TextBoxSearch}" IsEnabled="False" KeyDown="txtArticleId_KeyDown"/>
                <Button x:Name="btnArticleIdSrh"  Grid.Column="2"  Click="btnCustomer_Click" IsEnabled="False">
                    <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                </Button>
            </Grid>

            <Grid Grid.Column="2" Grid.Row="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="chkInstallLocation_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkInstallLocationSrh" Checked="chkInstallLocation_Checked" Unchecked="chkInstallLocation_UnChecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="현 장 명" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>

                    </StackPanel>
                </Label>
                <TextBox x:Name="txtInstallLocationSrh" Grid.Column="1" Style="{DynamicResource TextBoxSearch}" IsEnabled="False"/>
                <Button x:Name="btnInstallLocationSrh"  Grid.Column="2"  Click="btnInstallLocation_Click" IsEnabled="False" Visibility="Hidden">
                    <Image Source="{DynamicResource btiPlusFind}" Style="{DynamicResource imgPlusFind}"/>
                </Button>
            </Grid>



            <Grid Grid.Column="3" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="63*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="chkCloseYN_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkCloseYN" Checked="chkCloseYN_Checked" Unchecked="chkCloseYN_UnChecked"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="마감건 포함" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>

                    </StackPanel>
                </Label>
            </Grid>

            <!--CPO 정산-->
            <Grid Grid.Column="3" Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10*" />
                    <ColumnDefinition Width="13*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label x:Name="lblCpoCalcuDateSrh"  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="lblCpoCalcuDateSrh_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkCpoCalcuDateSrh" Click="chkCpoCalcuDateSrh_Click"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="CPO정산" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <DatePicker x:Name="dtpCpoCalcuSdateSrh" Grid.Column="1" Style="{DynamicResource DatePickerSearch}" />
                <DatePicker x:Name="dtpCpoCalcuEdateSrh" Grid.Column="2" Style="{DynamicResource DatePickerSearch}" />
            </Grid>

            <!--시공사정산-->
            <Grid Grid.Column="3" Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="10*" />
                    <ColumnDefinition Width="13*" />
                    <ColumnDefinition Width="13*" />
                </Grid.ColumnDefinitions>

                <Label x:Name="lblConstrCalcuDateSrh"  Grid.Column="0" Grid.Row="0" Style="{DynamicResource LabelSearch}" MouseLeftButtonUp="lblConstrCalcuDateSrh_MouseLeftButtonUp">
                    <StackPanel Style="{DynamicResource StackPanelInLabel}">
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <CheckBox x:Name="chkConstrCalcuDateSrh" Click="chkConstrCalcuDateSrh_Click"/>
                        </Viewbox>
                        <Viewbox Style="{DynamicResource ViewBoxSetMaxMinHeight}">
                            <TextBlock Text="시공사정산" Style="{DynamicResource TextBlockInViewBoxSearch}"/>
                        </Viewbox>
                    </StackPanel>
                </Label>
                <DatePicker x:Name="dtpConstrCalcuSdateSrh" Grid.Column="1" Style="{DynamicResource DatePickerSearch}" />
                <DatePicker x:Name="dtpConstrCalcuEdateSrh" Grid.Column="2" Style="{DynamicResource DatePickerSearch}" />
            </Grid>

            <Grid Grid.Column="3" Grid.Row="3" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Button x:Name="btnAttachFileSearch" Grid.Column="0" Content="첨부파일 조회"  Click="AttachFileSearch_Click" />
                <Button x:Name="btnGoOrder" Grid.Column="1" Content="수주등록 바로가기"  Click="GoOrderButton_Click" />   
            </Grid>
            


            <Grid Grid.Column="4" Grid.RowSpan="4" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Button x:Name="btnSearch" Grid.Column="0" Click="btnSearch_Click" Margin="1">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiSearch}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblSearch},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                    <Button x:Name="btnClose" Grid.Column="1" Click="btnClose_Click" Margin="1">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiClose}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblClose},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                </Grid>
                <Grid Grid.Row="1">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Button x:Name="btnPrint" Grid.Column="0" Click="btnPrint_Click" Margin="1">
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiPrint}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblPrint},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                        <Button.ContextMenu>
                            <ContextMenu x:Name="menu">
                                <MenuItem x:Name="menuSeeAhead" Header="미리보기" Click="menuSeeAhead_Click" ></MenuItem>
                                <MenuItem x:Name="menuRightPrint" Header="바로인쇄" Click="menuRightPrint_Click" ></MenuItem>
                                <MenuItem x:Name="menuClose" Header="닫   기" Click="menuClose_Click" ></MenuItem>
                            </ContextMenu>
                        </Button.ContextMenu>
                    </Button>
                    <Button x:Name="btnExcel" Grid.Column="1" Click="btnExcel_Click" Margin="1" >
                        <StackPanel Orientation="Horizontal">
                            <Image Source="{DynamicResource btiExcel}" Style="{DynamicResource imgSize}" VerticalAlignment="Center"/>
                            <TextBlock Text="{Binding Source={StaticResource tblExcel},Mode=OneWay,Path=Text}" VerticalAlignment="Center"/>
                        </StackPanel>
                    </Button>
                </Grid>
                <Label x:Name="lblMsg" Grid.Row="3" Grid.Column="0" Margin="1" Grid.ColumnSpan="2" Background="Yellow"
                          Foreground="Red" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"
                           Visibility="Hidden">
                    <TextBlock x:Name="tbkMsg" Margin="1" Text="엑셀을 내보내는 중" />
                </Label>
                <Grid>
                    
                </Grid>
            </Grid>
        </Grid>
        <!-- //전체 중 상단 -->

        <!--하단 레이아웃 나누기-->
        <Grid Grid.Row="1" >
            <Grid Grid.Column="0" >
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid Grid.Row="0"  >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="5*" />
                        <RowDefinition Height="50*" />
                    </Grid.RowDefinitions>
                    <ScrollViewer  Grid.Column="0" x:Name="dgdMainHeaderSh" HorizontalScrollBarVisibility="Hidden"  
                              ScrollViewer.CanContentScroll="True" VerticalScrollBarVisibility="Hidden"
                                ScrollChanged="HeaderScrollViewer_ScrollChanged" >
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.Resources>
                                <Style TargetType="DataGridColumnHeader">
                                    <Setter Property="Background" Value="#365fb1" />
                                    <Setter Property="BorderBrush" Value="White" />
                                    <Setter Property="Foreground" Value="White" />
                                    <Setter Property="BorderThickness" Value="0.7" />
                                    <Setter Property="FontWeight" Value="Thin" />
                                    <Setter Property="FontSize" Value="12"/>
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                </Style>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <!--순-->
                                <ColumnDefinition Width="{Binding ElementName=Num, Path=ActualWidth}" />
                                <!--마감여부-->
                                <ColumnDefinition Width="{Binding ElementName=CLoseYn, Path=ActualWidth}" />
                                <!--수주번호-->
                                <ColumnDefinition Width="{Binding ElementName=orderid, Path=ActualWidth}" />
                                <!--접수일자-->
                                <ColumnDefinition Width="{Binding ElementName=acptDate, Path=ActualWidth}" />
                                <!--견적번호-->
                                <ColumnDefinition Width="{Binding ElementName=EstID, Path=ActualWidth}" />

                                <!--영업회사-->
                                <ColumnDefinition Width="{Binding ElementName=saleCustomName, Path=ActualWidth}" />
                                <!--운영회사-->
                                <ColumnDefinition Width="{Binding ElementName=managerCustomName, Path=ActualWidth}" />
                                <!--실사업체-->
                                <ColumnDefinition Width="{Binding ElementName=searchCustomName, Path=ActualWidth}" />
                                <!--지역구분-->
                                <ColumnDefinition Width="{Binding ElementName=zoneGbnName, Path=ActualWidth}" />
                                <!--운영사접수일-->
                                <ColumnDefinition Width="{Binding ElementName=manageCustomAcptDate, Path=ActualWidth}" />

                                <!--운영사컨펌완료일-->
                                <ColumnDefinition Width="{Binding ElementName=manageCustomConfirmDate, Path=ActualWidth}"/>

                                <!--==================================절취선=================================-->
                                <!--■국소정보■-->
                                <!--CPO 정산-->
                                <ColumnDefinition Width="{Binding ElementName=cpoCalcuDate, Path=ActualWidth}" />
                                <!--시공사 정산-->
                                <ColumnDefinition Width="{Binding ElementName=constrCalcuDate, Path=ActualWidth}" />
                                <!--현장명-->
                                <ColumnDefinition Width="{Binding ElementName=installLocation, Path=ActualWidth}" />
                                <!--현장주소-->
                                <ColumnDefinition Width="{Binding ElementName=installLocationAddress, Path=ActualWidth}" />
                                <!--현장연락처-->
                                <ColumnDefinition Width="{Binding ElementName=InstallLocationPhone, Path=ActualWidth}" />
                                <!--거점명-->
                                <ColumnDefinition Width="{Binding ElementName=installLocationPart, Path=ActualWidth}" />
                                <!--세대수-->
                                <ColumnDefinition Width="{Binding ElementName=houseHoldCount, Path=ActualWidth}" />

                                <!--주차면수-->
                                <ColumnDefinition Width="{Binding ElementName=carParkingCount, Path=ActualWidth}" />
                                <!--전기차수-->
                                <ColumnDefinition Width="{Binding ElementName=electrCarCount, Path=ActualWidth}" />
                                <!--기설충전기수-->
                                <ColumnDefinition Width="{Binding ElementName=alReadyChargeCount, Path=ActualWidth}" />
                                <!--충전기신청수-->
                                <ColumnDefinition Width="{Binding ElementName=reqChargeCount, Path=ActualWidth}" />
                                <!--기설충전기운영사-->
                                <ColumnDefinition Width="{Binding ElementName=alreadyManageCustomName, Path=ActualWidth}" />

                                <!--국소특이사항-->
                                <ColumnDefinition Width="{Binding ElementName=installLocationComments, Path=ActualWidth}" />
                                <!--계약기간-->
                                <ColumnDefinition Width="{Binding ElementName=contractToDate, Path=ActualWidth}" />
                                <!--개통요청일-->
                                <ColumnDefinition Width="{Binding ElementName=openReqDate, Path=ActualWidth}" />
                                <!--개통일-->
                                <ColumnDefinition Width="{Binding ElementName=openDate, Path=ActualWidth}" />
                                <!--상담담당자-->
                                <ColumnDefinition Width="{Binding ElementName=damdangjaName, Path=ActualWidth}" />

                                <!--상담담당자연락처-->
                                <ColumnDefinition Width="{Binding ElementName=damdangjaPhone, Path=ActualWidth}" />
                                <!--상담담당자eMail-->
                                <ColumnDefinition Width="{Binding ElementName=damdangjaEMail, Path=ActualWidth}" />
                                <!--비고-->
                                <ColumnDefinition Width="{Binding ElementName=installLocationAddComments, Path=ActualWidth}" />

                                <!--==================================절취선=================================-->
                                <!--■영업회사■-->
                                <!--영업담당자(연락처)-->
                                <ColumnDefinition Width="{Binding ElementName=saledamdangjaPhone, Path=ActualWidth}" />
                                <!--영업부담금추가공사-->
                                <ColumnDefinition Width="{Binding ElementName=saleCustomAddWork, Path=ActualWidth}" />
                                <!--사은품정보-->
                                <ColumnDefinition Width="{Binding ElementName=salegift, Path=ActualWidth}" />

                                <!--==================================절취선=================================-->
                                <!--■기기 및 액세사리 정보■-->
                                <!--모델명-->
                                <ColumnDefinition Width="{Binding ElementName=article, Path=ActualWidth}" />
                                <!--충전기발주-->
                                <!--<ColumnDefinition Width="{Binding ElementName=chargeOrderDate, Path=ActualWidth}" />-->
                                <!--충전기입고-->
                                <!--<ColumnDefinition Width="{Binding ElementName=chargeInwareDate, Path=ActualWidth}" />-->
                                <!--충전기입고지정보-->
                                <ColumnDefinition Width="{Binding ElementName=chargeInwareLocation, Path=ActualWidth}" />

                                <!--스탠드&캐노피 발주업체 -> 자재발주업체로 이름 변경-->
                                <ColumnDefinition Width="{Binding ElementName=canopyReqCustom, Path=ActualWidth}" />
                                <!--헬멧형-->
                                <ColumnDefinition Width="{Binding ElementName=chargeModelHelmat, Path=ActualWidth}" />
                                <!--실내폴-->
                                <ColumnDefinition Width="{Binding ElementName=chargeModelinloc, Path=ActualWidth}" />
                                <!--일체형-->
                                <ColumnDefinition Width="{Binding ElementName=chargeModelOneBody, Path=ActualWidth}" />
                                <!--스탠드발주일 -> 발주일로 변경-->
                                <ColumnDefinition Width="{Binding ElementName=chargeStandReqDate, Path=ActualWidth}" />

                                <!--스탠드입고일 -> 입고일로 변경--> 
                                <ColumnDefinition Width="{Binding ElementName=chargeStandInwareDate, Path=ActualWidth}" />
                                <!--입고수량-->
                                <ColumnDefinition Width="{Binding ElementName=chargeInwareQty, Path=ActualWidth}" />
                                <!--캐노피&스탠드 입고지정보-->
                                <ColumnDefinition Width="{Binding ElementName=mtrCanopyInwareInfo, Path=ActualWidth}" />
                                <!--캐노피&스탠드 발주금-->
                                <ColumnDefinition Width="{Binding ElementName=mtrCanopyOrderAmount, Path=ActualWidth}" />
                                <!--비고-->
                                <ColumnDefinition Width="{Binding ElementName=comments, Path=ActualWidth}" />

                                <!--==================================절취선=================================-->
                                <!--■시공및 실사정보■-->
                                <!--실사요청일-->
                                <ColumnDefinition Width="{Binding ElementName=searchReqDate, Path=ActualWidth}" />
                                <!--실사일-->
                                <ColumnDefinition Width="{Binding ElementName=searchDate, Path=ActualWidth}" />
                                <!--실사횟수-->
                                <ColumnDefinition Width="{Binding ElementName=searchQty, Path=ActualWidth}" />
                                <!--실사자료접수일-->
                                <ColumnDefinition Width="{Binding ElementName=searchDataAcptDate, Path=ActualWidth}" />
                                <!--거점수-->
                                <ColumnDefinition Width="{Binding ElementName=installLocationCount, Path=ActualWidth}" />

                                <!--전기조달방식-->
                                <ColumnDefinition Width="{Binding ElementName=electrDeliveryMethod, Path=ActualWidth}" />
                                <!--감리배치여부-->
                                <ColumnDefinition Width="{Binding ElementName=inspectionNeedYN, Path=ActualWidth}" />
                                <!--추가공사비(시공사부담)-->
                                <ColumnDefinition Width="{Binding ElementName=addConstructCostSearch, Path=ActualWidth}" />
                                <!--추가공사비(EVC부담)-->
                                <ColumnDefinition Width="{Binding ElementName=addConstructCost, Path=ActualWidth}" />
                                <!--실사처리비고-->
                                <ColumnDefinition Width="{Binding ElementName=searchComments, Path=ActualWidth}" />

                                <!--==================================절취선=================================-->
                                <!--■공단사항■-->
                                <!--공단접수번호-->
